<h1><%= userId %>님의 최근 지출 내역</h1>

<table>
  <thead>
    <tr>
      <th>날짜</th>
      <th>카테고리</th>
      <th>메모</th>
      <th>금액</th>
      <th>감정적 소비</th>
      <th>소비 피드백</th>
    </tr>
  </thead>
  <tbody>
    <% results.forEach(item => { %>
    <tr>
      <td><%= item.spent_at %></td>
      <td><%= item.category_name %></td>
      <td><%= item.memo %></td>
      <td><%= item.amount.toLocaleString() %>원</td>
      <td>
        <% if (item.is_emotional === 1) { %>
          ✅ (<%= item.emotional_reason %>)
        <% } else if (item.is_emotional === 0) { %>
          ❌
        <% } else { %>
          -
        <% } %>
      </td>
      <td>
        <% if (item.feedback) { %>
          <%= item.feedback %>
        <% } else { %>
          -
        <% } %>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
